{"components/themevale/homepage/component/ajax-products-by-category-id-3":"<section class=\"themevale-section background-gray {{padding_top}} {{padding_bottom}}\">\r\n    <div class=\"container\">\r\n        {{#each categories}}\r\n            {{#if id '==' ../category_id}}\r\n                <div class=\"themevale_productsByCategoryId-list\">\r\n                    <h2 class=\"page-heading no-border text-left\"><a href=\"{{url}}\">{{name}}</a></h2>\r\n                    <div data-themevale-products-by-category-id=\"{{url}}\">\r\n                        {{> components/themevale/homepage/component/product-loading theme_settings=../../theme_settings}}\r\n                    </div>\r\n                </div>\r\n            {{else}}\r\n                {{#each children}}\r\n                    {{#if id '==' ../../../category_id}}\r\n                        <div class=\"themevale_productsByCategoryId-list\">\r\n                            <h2 class=\"page-heading no-border text-left\"><a href=\"{{url}}\">{{name}}</a></h2>\r\n                            <div data-themevale-products-by-category-id=\"{{url}}\">\r\n                                {{> components/themevale/homepage/component/product-loading theme_settings=../../../../theme_settings}}\r\n                            </div>\r\n                        </div>\r\n                    {{else}}\r\n                        {{#each children}}\r\n                            {{#if id '==' ../../../../../category_id}}\r\n                                <div class=\"themevale_productsByCategoryId-list\">\r\n                                    <h2 class=\"page-heading no-border text-left\"><a href=\"{{url}}\">{{name}}</a></h2>\r\n                                    <div data-themevale-products-by-category-id=\"{{url}}\">\r\n                                        {{> components/themevale/homepage/component/product-loading theme_settings=../../../../../../theme_settings}}\r\n                                    </div>\r\n                                </div>\r\n                            {{/if}}\r\n                        {{/each}}\r\n                    {{/if}}\r\n                {{/each}}\r\n            {{/if}}\r\n        {{/each}}\r\n    </div>\r\n</section>\n","components/themevale/homepage/component/product-loading":"<div class=\"themevale_productLoading\">\r\n    <img src=\"{{cdn 'img/loading.svg'}}\" alt=\"loading\" title=\"loading\" />\r\n</div>\r\n"}
