{"components/products/add-to-cart":"<div class=\"form-field form-field--increments\">\n    <label class=\"form-label form-label--alternate\"\n           for=\"qty[]\">{{lang 'products.quantity'}}</label>\n    <div class=\"form-increment\" data-quantity-change>\n        <button class=\"button button--icon\" data-action=\"dec\">\n            <i class=\"icon\" aria-hidden=\"true\">-</i>\n        </button>\n        <input class=\"form-input form-input--incrementTotal\"\n               id=\"qty[]\"\n               name=\"qty[]\"\n               type=\"tel\"\n               value=\"{{#if product.min_purchase_quantity}}{{product.min_purchase_quantity}}{{else}}1{{/if}}\"\n               data-quantity-min=\"{{product.min_purchase_quantity}}\"\n               data-quantity-max=\"{{product.max_purchase_quantity}}\"\n               min=\"1\"\n               pattern=\"[0-9]*\"\n               aria-live=\"polite\">\n        <button class=\"button button--icon\" data-action=\"inc\">\n            <i class=\"icon\" aria-hidden=\"true\">+</i>\n        </button>\n    </div>\n</div>\n    <div class=\"custom-buttons-add-to\">\n        <a class=\"btn-nope active\" id=\"nope-once\" onclick=\"closeSubscribe();\">\n            <span>ONCE</span><br>\n            Nope, just one time please\n        </a>\n        \n        <a class=\"btn-subscribe\" id=\"yup-sub\" onclick=\"openSubscribe();\">\n            <span>SUBSCRIBE</span><br>\n            Yup, I would like this item to recur by subscription & save 10%.\n        \n        </a>\n    </div>\n    {{#each product.options}}\t\n    {{#or (if this.display_name '===' 'Subscribe & Save') }}\n        <div id=\"subscription-panel\" style=\"display: none;\">\n            <div id=\"subscription-opions\">\n                <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n                    Choose:\n                </label>\n                <div class=\"subscription-options-flex\">\n                    <div id=\"weekly-button\" rel=\"\" class=\"sub-buttons\"><img src=\"https://cdn11.bigcommerce.com/s-zlze5h6hq9/product_images/uploaded_images/byweeks.png?t=1595319938&_ga=2.180444835.545570939.1595290657-1335674609.1584404257\" onclick=\"openWeekly();\" id=\"weeklyImg\"></div>\n                    <div id=\"monthly-button\" rel=\"\" class=\"sub-buttons\"><img src=\"https://cdn11.bigcommerce.com/s-zlze5h6hq9/product_images/uploaded_images/bymonths.png?t=1595319937&_ga=2.180444835.545570939.1595290657-1335674609.1584404257\" onclick=\"openMonthly();\" id=\"monthlyImg\"></div>\n                </div>\n            </div>\n            <div class=\"form-field\" data-product-attribute=\"set-select\" id=\"subscription-choices\" style=\"display: none;\">\n\n                <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\n                    {{ this.display_name }}:\n                \n                    {{#if required}}\n                        <small>{{lang 'common.required'}}</small>\n                    {{/if}}\n                </label>\n                \n                <div id=\"weekly-choices\" style=\"display: none;\">\n                    {{#each this.values}}\n                    <label id=\"img-test\" data-product-attribute-value=\"{{id}}\" class=\"form-label radio-img subscription-check\" for=\"attribute_radio_{{../id}}_{{id}}\">\n                        <input\n                            class=\"form-radio radio-img-test unselect-data\"\n                            type=\"radio\"\n                            id=\"attribute_radio_{{../id}}_{{id}}\"\n                            name=\"attribute[{{../id}}]\"\n                            value=\"{{id}}\"\n                            {{#if selected}}\n                                checked\n                                data-default\n                            {{/if}}\n                            {{#if ../required}}required{{/if}}>\n                            \n                            {{#is this.label 'Send every 1 week'}}\n                \t\t\t\t<img id=\"week1Img\" style=\"display: none;\" class=\"weekly-data\" onclick=\"weekRadio1();\" src=\"https://cdn11.bigcommerce.com/s-zlze5h6hq9/product_images/uploaded_images/week1.png?t=1594884765&_ga=2.243466565.1541745109.1594859328-1335674609.1584404257\" />\n                            {{/is}}\n                            {{#is this.label 'Send every 2 weeks'}}\n                \t\t\t\t<img id=\"week2Img\" style=\"display: none;\" class=\"weekly-data\" onclick=\"weekRadio2();\" src=\"https://cdn11.bigcommerce.com/s-zlze5h6hq9/product_images/uploaded_images/week2.png?t=1594884765&_ga=2.243466565.1541745109.1594859328-1335674609.1584404257\" />\n                            {{/is}}\n                            {{#is this.label 'Send every 3 weeks'}}\n                \t\t\t\t<img id=\"week3Img\" style=\"display: none;\" class=\"weekly-data\" onclick=\"weekRadio3();\" src=\"https://cdn11.bigcommerce.com/s-zlze5h6hq9/product_images/uploaded_images/week3.png?t=1594884766&_ga=2.243466565.1541745109.1594859328-1335674609.1584404257\" />\n                            {{/is}}\n                            {{#is this.label 'Send every 4 weeks'}}\n                \t\t\t\t<img id=\"week4Img\" style=\"display: none;\" class=\"weekly-data\" onclick=\"weekRadio4();\" src=\"https://cdn11.bigcommerce.com/s-zlze5h6hq9/product_images/uploaded_images/week4.png?t=1594884766&_ga=2.243466565.1541745109.1594859328-1335674609.1584404257\" />\n                            {{/is}}\n                            {{#is this.label 'Send every 5 weeks'}}\n                \t\t\t\t<img id=\"week5Img\" style=\"display: none;\" class=\"weekly-data\" onclick=\"weekRadio5();\" src=\"https://cdn11.bigcommerce.com/s-zlze5h6hq9/product_images/uploaded_images/week5.png?t=1594884767&_ga=2.243466565.1541745109.1594859328-1335674609.1584404257\" />\n                            {{/is}}\n                            {{#is this.label 'Send every 6 weeks'}}\n                \t\t\t\t<img id=\"week6Img\" style=\"display: none;\" class=\"weekly-data\" onclick=\"weekRadio6();\" src=\"https://cdn11.bigcommerce.com/s-zlze5h6hq9/product_images/uploaded_images/week6.png?t=1594884767&_ga=2.243466565.1541745109.1594859328-1335674609.1584404257\" />\n                            {{/is}}\n                            \n                            {{#is this.label 'Send every 1 month'}}\n                \t\t\t\t<img id=\"month1Img\" style=\"display: none;\" class=\"monthly-data\" onclick=\"monthRadio1();\" src=\"https://cdn11.bigcommerce.com/s-zlze5h6hq9/product_images/uploaded_images/month1.png?t=1594884946&_ga=2.208815058.1541745109.1594859328-1335674609.1584404257\" />\n                            {{/is}}\n                            {{#is this.label 'Send every 2 months'}}\n                \t\t\t\t<img id=\"month2Img\" style=\"display: none;\" class=\"monthly-data\" onclick=\"monthRadio2();\" src=\"https://cdn11.bigcommerce.com/s-zlze5h6hq9/product_images/uploaded_images/month2.png?t=1594884946&_ga=2.208815058.1541745109.1594859328-1335674609.1584404257\" />\n                            {{/is}}\n                            {{#is this.label 'Send every 3 months'}}\n                \t\t\t\t<img id=\"month3Img\" style=\"display: none;\" class=\"monthly-data\" onclick=\"monthRadio3();\" src=\"https://cdn11.bigcommerce.com/s-zlze5h6hq9/product_images/uploaded_images/month3.png?t=1594884947&_ga=2.208815058.1541745109.1594859328-1335674609.1584404257\" />\n                            {{/is}}\n                            {{#is this.label 'Send every 4 months'}}\n                \t\t\t\t<img id=\"month4Img\" style=\"display: none;\" class=\"monthly-data\" onclick=\"monthRadio4();\" src=\"https://cdn11.bigcommerce.com/s-zlze5h6hq9/product_images/uploaded_images/month4.png?t=1594884948&_ga=2.208815058.1541745109.1594859328-1335674609.1584404257\" />\n                            {{/is}}\n                            {{#is this.label 'Send every 5 months'}}\n                \t\t\t\t<img id=\"month5Img\" style=\"display: none;\" class=\"monthly-data\" onclick=\"monthRadio5();\" src=\"https://cdn11.bigcommerce.com/s-zlze5h6hq9/product_images/uploaded_images/month5.png?t=1594884949&_ga=2.208815058.1541745109.1594859328-1335674609.1584404257\" />\n                            {{/is}}\n                            {{#is this.label 'Send every 6 months'}}\n                \t\t\t\t<img id=\"month6Img\" style=\"display: none;\" class=\"monthly-data\" onclick=\"monthRadio6();\" src=\"https://cdn11.bigcommerce.com/s-zlze5h6hq9/product_images/uploaded_images/month6.png?t=1594884950&_ga=2.208815058.1541745109.1594859328-1335674609.1584404257\" />\n                            {{/is}}\n                    </label>\n                    {{/each}}\n                </div>\n            </div>\n        </div>\n    {{/or}}\n    {{/each}}\n\n<div class=\"alertBox productAttributes-message\" style=\"display:none\">\n    <div class=\"alertBox-column alertBox-icon\">\n        <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\n    </div>\n    <p class=\"alertBox-column alertBox-message\"></p>\n</div>\n{{#or customer (if theme_settings.restrict_to_login '!==' true)}}\n    <div class=\"form-action cart-product-page-action\">\n        <input id=\"form-action-addToCart\" data-wait-message=\"{{lang 'products.adding_to_cart'}}\" class=\"button\" type=\"submit\"\n            value=\"{{#if product.pre_order}}{{lang 'products.pre_order'}}{{else}}{{lang 'products.add_to_cart'}}{{/if}}\">\n    </div>\n     {{{snippet 'product_addtocart'}}}\n{{/or}}\n"}
