{"components/common/navigation-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n            <p class=\"navPage-subMenu-action navPages-action\">\n                <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                    <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                </span>\n                <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n            </p>\n        </li>\n        {{#each children}}\n            {{#if children}}\n                <li class=\"navPage-subMenu-item-child\">\n                    <p class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu\">\n                        <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n                        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                            <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                        </span>\n                    </p>\n                    {{> components/common/navigation-dropdown}}\n                </li>\n            {{else}}\n                <li class=\"navPage-subMenu-item-child navPages-action-end\">\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                </li>\n            {{/if}}\n        {{/each}}\n    </ul>\n</div>\n","components/common/navigation-list-alternate":"{{#if children}}\n    <p class=\"navPages-action navPages-action-depth-max has-subMenu is-root\">\n        <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n            <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n        </span>\n    </p>\n    {{> components/common/navigation-dropdown}}\n{{else}