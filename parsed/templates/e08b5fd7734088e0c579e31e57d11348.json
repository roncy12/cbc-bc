{"components/common/navigation-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n            <p class=\"navPage-subMenu-action navPages-action\">\n                <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                    <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                </span>\n                <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n            </p>\n        </li>\n        {{#each children}}\n            {{#if children}}\n                <li class=\"navPage-subMenu-item-child\">\n                    <p class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu\">\n                        <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n                        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                            <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                        </span>\n                    </p>\n                    {{> components/common/navigation-dropdown}}\n                </li>\n            {{else}}\n                <li class=\"navPage-subMenu-item-child navPages-action-end\">\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                </li>\n            {{/if}}\n        {{/each}}\n    </ul>\n</div>\n","components/common/navigation-list-alternate":"{{#if children}}\n    <p class=\"navPages-action navPages-action-depth-max has-subMenu is-root\">\n        <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n            <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n        </span>\n    </p>\n    {{> components/common/navigation-dropdown}}\n{{else}}\n    <a class=\"navPages-action\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n{{/if}}\n","components/common/navigation-menu2":"<div class=\"navPages-container\" id=\"menu\" data-menu>\r\n    <div class=\"themevale_allCategories {{#if theme_settings.themevale_allCategories_open}}is-open{{/if}}\">\r\n        <p class=\"themevale_allCategories-text\">\r\n            <svg class=\"icon\"><use xlink:href=\"#menu-mobile\"></use></svg>\r\n            <span class=\"text\">{{lang 'category.label'}}</span>\r\n        </p>\r\n        <div class=\"themevale_allCategories-dropdown\">\r\n            <ul class=\"navPages-list\">\r\n                {{#each categories}}\r\n                    {{#or (if id '==' 167) (if id '==' 168) (if id '==' 169) (if id '==' 170)}}\r\n                    {{else}}\r\n                        <li class=\"navPages-item\">\r\n                            {{#if children}}\r\n                                <p class=\"navPages-action navPages-action-depth-max has-subMenu is-root\">\r\n                                    {{#if image}}\r\n                                        <img class=\"image-icon\" src=\"{{getImage image 'categories_size'}}\" alt=\"\">\r\n                                    {{/if}}\r\n                                    <a class=\"text\" href=\"{{url}}\">{{name}}</a>\r\n                                    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                                        <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                                    </span>\r\n                                </p>\r\n                                {{> components/common/navigation-dropdown}}\r\n                            {{else}}\r\n                                <a class=\"navPages-action\" href=\"{{url}}\">\r\n                                    {{#if image}}\r\n                                        <img class=\"image-icon\" src=\"{{getImage image 'categories_size'}}\" alt=\"\">\r\n                                    {{/if}}\r\n                                    <span class=\"text\">{{name}}</span>\r\n                                </a>\r\n                            {{/if}}\r\n                        </li>\r\n                    {{/or}}\r\n                {{/each}}\r\n            </ul>\r\n        </div>\r\n    </div>\r\n    <nav class=\"navPages\">\r\n        <ul class=\"navPages-list navPages-list-depth-max\">\r\n            {{#each categories}}\r\n                {{#or (if id '==' 167) (if id '==' 168) (if id '==' 169) (if id '==' 170)}}\r\n                    <li class=\"navPages-item navPages-item-custom\">\r\n                        {{> components/common/navigation-list-alternate}}\r\n                    </li>\r\n                {{else}}\r\n                    <li class=\"navPages-item navPages-item-hidden\">\r\n                        {{> components/common/navigation-list-alternate}}\r\n                    </li>\r\n                {{/or}}\r\n            {{/each}}\r\n            {{#unless theme_settings.hide_content_navigation}}\r\n                {{#each pages}}\r\n                    <li class=\"navPages-item navPages-item-page\">\r\n                        {{> components/common/navigation-list-alternate}}\r\n                    </li>\r\n                 {{/each}}\r\n            {{/unless}}\r\n        </ul>\r\n\r\n        <ul class=\"navPages-list navPages-list--user\">\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\" href=\"{{urls.account.wishlists.all}}\">\r\n                    <span class=\"text\"><svg class=\"icon\"><use xlink:href=\"#icon-heart-2\"/></svg><span>{{lang 'account.mobile_nav.wishlists'}}</span></span>\r\n                </a>\r\n            </li>\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\" href=\"/contact-us/\">\r\n                    <span class=\"text\"><svg class=\"icon\"><use xlink:href=\"#icon-location\"/></svg><span>Store Locator</span></span>\r\n                </a>\r\n            </li>\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\" href=\"tel:{{settings.phone_number}}\">\r\n                    <span class=\"text\"><svg class=\"icon\"><use xlink:href=\"#icon-telephone\"/></svg><span>Contact us: {{settings.phone_number}}</span></span>\r\n                </a>\r\n            </li>\r\n            {{#if settings.gift_certificates_enabled}}\r\n                <li class=\"navPages-item\">\r\n                    <a class=\"navPages-action\" href=\"{{urls.gift_certificate.purchase}}\">\r\n                        <span class=\"text\"><svg class=\"icon\"><use xlink:href=\"#icon-gift\"/></svg><span>{{lang 'common.gift_cert'}}</span></span>\r\n                    </a>\r\n                </li>\r\n            {{/if}}\r\n            {{#if currency_selector.currencies.length '>' 1}}\r\n                <li class=\"navPages-item\">\r\n                    <p class=\"navPages-action has-subMenu\">\r\n                        <a class=\"text\" href=\"javascript:void(0)\">{{{currency_selector.active_currency_flag}}} {{lang 'common.currency' code=currency_selector.active_currency_code}}</a>\r\n                        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                            <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                        </span>\r\n                    </p>\r\n                    <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                        <ul class=\"navPage-subMenu-list\">\r\n                            <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\r\n                                <p class=\"navPage-subMenu-action navPages-action\">\r\n                                    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                                        <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                                        <span>back</span>\r\n                                    </span>\r\n                                    <a class=\"text\" href=\"javascript:void(0)\">{{{currency_selector.active_currency_flag}}} {{lang 'common.currency' code=currency_selector.active_currency_code}}</a>\r\n                                </p>\r\n                            </li>\r\n                            {{#each currency_selector.currencies}}\r\n                            <li class=\"navPage-subMenu-item-child navPages-action-end\">\r\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\r\n                                    <span class=\"text\">\r\n                                    {{#if is_active}}\r\n                                        {{{flag}}} {{name}}\r\n                                    {{else}}\r\n                                        {{{flag}}} {{name}}\r\n                                    {{/if}}\r\n                                    </span>\r\n                                </a>\r\n                            </li>\r\n                            {{/each}}\r\n                        </ul>\r\n                    </div>\r\n                </li>\r\n            {{/if}}\r\n            <li class=\"navPages-item\">\r\n                {{> components/common/social-links}}\r\n            </li>\r\n        </ul>\r\n    </nav>\r\n</div>\r\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                {{#if display_name '===' 'google+'}}\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\n                        <svg><use xlink:href=\"#icon-google\" /></svg>\n                    </a>\n                {{else}}\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n"}
