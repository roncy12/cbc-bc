v>\r\n                    </div>\r\n                    <div class=\"navUser-item navUser-item--account\">\r\n                        <a class=\"navUser-action accountMobile\" href=\"javascript:void(0)\">\r\n                            <div class=\"navUser-icon\">\r\n                                <svg class=\"icon\"><use xlink:href=\"#icon-user\"/></svg>\r\n                            </div>\r\n                        </a>\r\n                    </div>\r\n                    <div class=\"navUser-item navUser-item--wishList\">\r\n                        <a class=\"navUser-action\" href=\"{{urls.account.wishlists.all}}\">\r\n                            <div class=\"navUser-icon\">\r\n                                <svg class=\"icon\"><use xlink:href=\"#icon-heart-2\"></use></svg>\r\n                            </div>\r\n                        </a>\r\n                    </div>\r\n                    <div class=\"navUser-item navUser-item--cart themevale_cart\">\r\n                        <a class=\"navUser-action cartMobile\"\r\n                            data-cart-preview\r\n                            data-options=\"align:right\"\r\n                            href=\"{{urls.cart}}\">\r\n                            <div class=\"navUser-icon navUser-item-cartIcon\">\r\n                                <svg class=\"icon\"><use xlink:href=\"#icon-cart-2\"></use></svg>\r\n                            </div>\r\n                            <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{#if cart.items}}{{cart.quantity}}{{else}}0{{/if}}</span>\r\n                        </a>\r\n                    </div>\r\n                    <div class=\"navUser-item navUser-item--currency\">\r\n                        <a class=\"navUser-action currencyMobile\" href=\"javascript:void(0)\">\r\n                            <div class=\"navUser-icon\">\r\n                                <svg class=\"icon\"><use xlink:href=\"#menu-mobile\"/></svg>\r\n 