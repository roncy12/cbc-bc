{"components/common/currency-selector":"{{#if currency_selector.currencies.length '>' 1}}\n<a class=\"navUser-action navUser-action--currencySelector\" href=\"javascript:void(0)\">\n    <span class=\"image-flag\">{{{currency_selector.active_currency_flag}}}</span> \n    <span class=\"text-flag\">{{lang 'common.currency' code=currency_selector.active_currency_code}}</span>\n    <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i></a>\n<ul class=\"dropdown-menu\" id=\"currencySelection\">\n    {{#each currency_selector.currencies}}\n    <li class=\"dropdown-menu-item\">\n        <a href=\"{{{switch_url}}}\">\n            {{#if is_active}}\n            {{{flag}}} <strong>{{name}}</strong>\n            {{else}}\n            {{{flag}}} {{name}}\n            {{/if}}\n        </a>\n    </li>\n    {{/each}}\n</ul>\n{{/if}}\n","components/common/navigation-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n            <p class=\"navPage-subMenu-action navPages-action\">\n                <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                    <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                </span>\n                <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n            </p>\n        </li>\n        {{#each children}}\n            {{#if children}}\n                <li class=\"navPage-subMenu-item-child\">\n                    <p class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu\">\n                        <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n                        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                            <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                        </span