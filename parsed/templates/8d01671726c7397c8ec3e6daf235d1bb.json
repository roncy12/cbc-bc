{"components/common/geotrust-ssl-seal":"<table width=\"135\" border=\"0\" cellpadding=\"2\" cellspacing=\"0\" title=\"Click to Verify - This site chose GeoTrust SSL for secure e-commerce and confidential communications.\">\n    <tr>\n        <td width=\"135\" align=\"center\" valign=\"top\">\n            <script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name={{theme_settings.geotrust_ssl_common_name}}&amp;size={{theme_settings.geotrust_ssl_seal_size}}&amp;lang={{locale_name}}\"></script><br />\n            <a href=\"http://www.geotrust.com/ssl/\" target=\"_blank\"  style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\"></a>\n        </td>\n    </tr>\n</table>\n","components/common/payment-icons":"{{#with theme_settings}}\n    {{#or show_accept_amex show_accept_discover show_accept_mastercard show_accept_paypal show_accept_visa show_accept_sage show_accept_wallet}}\n        <div class=\"footer-payment-image\">\n            <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{cdn '/assets/img/home/stamped.png'}}\" alt=\"stamped\">\n            {{#if show_accept_visa}}\n                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-3tntjfynbx/product_images/uploaded_images/payment-1.png\" alt=\"payment-1\">\n            {{/if}}\n            {{#if show_accept_mastercard}}\n                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-3tntjfynbx/product_images/uploaded_images/payment-2.png\" alt=\"payment-2\">\n            {{/if}}\n            {{#if show_accept_amex}}\n                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-3tntjfynbx/product_images/uploaded_images/payment-3.png\" alt=\"payment-3\">\n            {{/if}}\n            {{#if show_accept_discover}}\n                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-3tntjfynbx/product_images/uploaded_images/payment-4.png\" alt=\"payment-4\">\n            {{/if}}\n            {{#if show_accept_sage}}\n                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-3tntjfynbx/product_images/uploaded_images/payment-8.png\" alt=\"payment-8\">\n            {{/if}}\n            {{#if show_accept_paypal}}\n                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-3tntjfynbx/product_images/uploaded_images/payment-5.png\" alt=\"payment-5\">\n            {{/if}}\n            {{#if show_accept_wallet}}\n                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-3tntjfynbx/product_images/uploaded_images/payment-9.png\" alt=\"payment-9\">\n            {{/if}}\n        </div>\n    {{/or}}\n{{/with}}\n","components/themevale/footer/footer-layout-5":"<footer class=\"footer footer-layout-3 {{#if theme_settings.footer-dark-layout}}themevale-footer-dark{{/if}}\">\r\n    <div class=\"themevale_footer-middle\">\r\n        <div class=\"container\">\r\n            <section class=\"footer-info themevale_footer-info col-7\">\r\n                <div class=\"footer-info-col footer-dropdownmobile\" data-section-type=\"footer-location\">\r\n                    <h5 class=\"footer-info-heading\">\r\n                        {{#if theme_settings.footer-dark-layout}}\r\n                            <img class=\"logo-image lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/logo-whitecolor.png\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n                        {{else}}\r\n                            <img class=\"logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n                        {{/if}}\r\n                        <svg class=\"icon-dropdown\"><use xlink:href=\"#icon-chevron-down\"></use></svg>\r\n                    </h5>\r\n                    <div class=\"footer-info-list\">\r\n                        <div class=\"store-address\">{{nl2br settings.address}}</div>\r\n                        <br>\r\n                        <div class=\"store-phone\">call us: {{settings.phone_number}}</div>\r\n                        <div class=\"store-email\"><a href=\"mailto:support@halothemes.com\">email: support@halothemes.com</a></div>\r\n                    </div>\r\n                    <div class=\"footer-info-list\"> \r\n                        {{#if social_media}}\r\n                            <ul class=\"socialLinks style-2 socialLinks--alt\">\r\n                                {{#each social_media}}\r\n                                    <li class=\"socialLinks-item\">\r\n                                        {{#if display_name '===' 'google+'}}\r\n                                            <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\r\n                                                <svg><use xlink:href=\"#icon-google\" /></svg>\r\n                                            </a>\r\n                                        {{else}}\r\n                                            <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\r\n                                                <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\r\n                                            </a>\r\n                                        {{/if}}\r\n                                    </li>\r\n                                {{/each}}\r\n                            </ul>\r\n                        {{/if}}\r\n                    </div>\r\n                </div>\r\n                <div class=\"footer-info-col footer-info-col--small footer-dropdownmobile\" data-section-type=\"footer-categories\">\r\n                    <h5 class=\"footer-info-heading\">\r\n                        <span>Shop</span>\r\n                        <svg class=\"icon-dropdown\"><use xlink:href=\"#icon-chevron-down\"></use></svg>\r\n                    </h5>\r\n                    <ul class=\"footer-info-list\">\r\n                        {{#each categories}}\r\n                            <li>\r\n                                <a href=\"{{url}}\">{{name}}</a>\r\n                            </li>\r\n                        {{/each}}\r\n                        <li>\r\n                            <a href=\"/lookbook/\">lookbook</a>\r\n                        </li>\r\n                    </ul>\r\n                </div>\r\n\r\n                <div class=\"footer-info-col footer-info-col--small footer-dropdownmobile\" data-section-type=\"footer-webPages\">\r\n                    <h5 class=\"footer-info-heading\">\r\n                        <span>Infomation</span>\r\n                        <svg class=\"icon-dropdown\"><use xlink:href=\"#icon-chevron-down\"></use></svg>\r\n                    </h5>\r\n                    <ul class=\"footer-info-list\">\r\n                        {{#each pages}}\r\n                            <li>\r\n                                <a href=\"{{url}}\">{{name}}</a>\r\n                            </li>\r\n                        {{/each}}\r\n                        <li>\r\n                            <a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a>\r\n                        </li>\r\n                    </ul>\r\n                </div>\r\n\r\n                <div class=\"footer-info-col footer-info-col--small footer-dropdownmobile\" data-section-type=\"footer-services\">\r\n                    <h5 class=\"footer-info-heading\">\r\n                        <span>Customer Service</span>\r\n                        <svg class=\"icon-dropdown\"><use xlink:href=\"#icon-chevron-down\"></use></svg>\r\n                    </h5>\r\n                    <ul class=\"footer-info-list\">\r\n                        <li><a href=\"#\">Search Terms</a></li>\r\n                        <li><a href=\"#\">Advanced Search</a></li>\r\n                        <li><a href=\"#\">Orders and Returns</a></li>\r\n                        <li><a href=\"/contact-us/\">Contact Us</a></li>\r\n                        <li><a href=\"/faqs/\">Theme FAQs</a></li>\r\n                        <li><a href=\"#\">Consultant</a></li>\r\n                        <li><a href=\"/contact-us/\">Store Locations</a></li>\r\n                    </ul>\r\n                </div>\r\n\r\n                <div class=\"footer-info-col\" data-section-type=\"newsletterSubscription\">\r\n                    <div class=\"item\"> \r\n                        {{#if settings.show_newsletter_box}}\r\n                            <h5 class=\"footer-info-heading\">{{lang 'newsletter.subscribe_3'}}</h5>\r\n                            <p class=\"description\">Receive our latest updates about our products and promotions.</p>\r\n                            <form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n                                <fieldset class=\"form-fieldset\">\r\n                                    <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n                                    <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n                                    <input type=\"hidden\" name=\"check\" value=\"1\">\r\n                                    <div class=\"form-field\">\r\n                                        <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n                                        <div class=\"form-prefixPostfix wrap\">\r\n                                            <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\r\n                                            <input class=\"button form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\r\n                                        </div>\r\n                                        {{#if settings.show_newsletter_summary }}\r\n                                            <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\r\n                                        {{/if}}\r\n                                    </div>\r\n                                </fieldset>\r\n                            </form>\r\n                        {{/if}}\r\n                    </div>\r\n                    <div class=\"item\">\r\n                        <div class=\"themevale_securityImage\">\r\n                            <ul>\r\n                                <li>\r\n                                    <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-3tntjfynbx/product_images/uploaded_images/security-1.png\" alt=\"\">\r\n                                </li>\r\n                                <li>\r\n                                    <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-3tntjfynbx/product_images/uploaded_images/security-2.png\" alt=\"\">\r\n                                </li>\r\n                                <li>\r\n                                    <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-3tntjfynbx/product_images/uploaded_images/security-3.png\" alt=\"\">\r\n                                </li>\r\n                                <li>\r\n                                    <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-3tntjfynbx/product_images/uploaded_images/security-4.png\" alt=\"\">\r\n                                </li>\r\n                            </ul>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </section>\r\n        </div>\r\n    </div>\r\n    <div class=\"themevale_footer-bottom layout-2\">\r\n        <div class=\"container\">\r\n            <div class=\"wrapper\">\r\n                <div class=\"item\">\r\n                    {{#if theme_settings.show_copyright_footer}}\r\n                        <div class=\"footer-copyright\">\r\n                            <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}} .</p>\r\n                        </div>\r\n                    {{/if}}\r\n\r\n                    {{#if theme_settings.show_powered_by}}\r\n                        <div class=\"footer-copyright\">\r\n                            <p class=\"powered-by\">Bigcommerce Themes & Templates by <a href=\"themevale.com\" rel=\"nofollow\">ThemeVale.com</a></p>\r\n                        </div>\r\n                    {{/if}}\r\n                </div>\r\n                <div class=\"item\">\r\n                    {{> components/common/payment-icons}}\r\n                    {{#if theme_settings.geotrust_ssl_common_name}}\r\n                        <div class=\"footer-geotrust-ssl-seal\">\r\n                            {{> components/common/geotrust-ssl-seal}}\r\n                        </div>\r\n                    {{/if}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</footer>\n"}
