{"components/products/options/set-radio":"<div class=\"form-field\" data-product-attribute=\"set-radio\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    {{#each this.values}}\n\t<label data-product-attribute-value=\"{{id}}\" class=\"form-label radio-img\" for=\"attribute_radio_{{../id}}_{{id}}\">\n        <input\n            class=\"form-radio radio-img-test\"\n            type=\"radio\"\n            id=\"attribute_radio_{{../id}}_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}>\n        <img src=\"{{cdn '/assets/img/'}}{{this.label}}.png\" /></label>\n    {{/each}}\n</div>\n"}
