{"components/themevale/newsletter":"<div id=\"themevale_newsletter\" class=\"hide\">\r\n    <div class=\"popup-overlay\"></div>\r\n    <div class=\"newsletterPopup\">\r\n        <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\" data-close-newsletter-popup>\r\n            <span aria-hidden=\"true\">&#215;</span>\r\n        </a>\r\n        <div class=\"newsletter-image\">\r\n            <img src=\"https://cdn11.bigcommerce.com/s-3tntjfynbx/product_images/uploaded_images/newletter.jpg\" alt=\"\">\r\n        </div>\r\n        <div class=\"newsletter-content\">\r\n            <h2 class=\"newsletter-heading\">Join Our Mailing List</h2>\r\n            <p class=\"newsletter-sub-heading\">Sign Up for exclusive updates, <br> new arrivals & insider-only discounts</p>\r\n            <div id=\"popupSubcribeForm\">\r\n                <form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\" id=\"popupSubcribeFormSubmit\">\r\n                    <fieldset class=\"form-fieldset\">\r\n                        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n                        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n                        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n                        <div class=\"form-field\">\r\n                            <div class=\"form-wrap\">\r\n                                <input class=\"form-input\" id=\"nl_email2\" name=\"nl_email\" type=\"email\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\r\n                                <input class=\"button button--border\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit_2'}}\">\r\n                            </div>\r\n                        </div>\r\n                    </fieldset>\r\n                </form>\r\n            </div>\r\n            <div class=\"newsletter-icons\">\r\n                {{#if social_media}}\r\n                    <ul class=\"socialLinks style-2 socialLinks--alt\">\r\n                        {{#each social_media}}\r\n                            <li class=\"socialLinks-item\">\r\n                                {{#if display_name '===' 'google+'}}\r\n                                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\r\n                                        <svg><use xlink:href=\"#icon-google\" /></svg>\r\n                                    </a>\r\n                                {{else}}\r\n                                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\r\n                                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\r\n                                    </a>\r\n                                {{/if}}\r\n                            </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                {{/if}}\r\n            </div>\r\n            <div class=\"newsletter-show-again\">\r\n                <form>\r\n                    <input id=\"do-not-show-again\" class=\"do-not-show-again\" type=\"checkbox\" name=\"don't show again\" value=\"1\"> \r\n                    <label for=\"do-not-show-again\">No, thanks</label>\r\n                </form>\r\n            </div>\r\n        </div>\r\n        \r\n    </div>\r\n</div>\r\n"}
