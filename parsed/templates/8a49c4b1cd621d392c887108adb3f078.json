                   </div>\r\n                                                    <div class=\"form-field\">\r\n                                                        <label class=\"form-label\" for=\"login_pass2\">{{lang 'login.field_password'}}</label>\r\n                                                        <input class=\"form-input\" id=\"login_pass2\" type=\"password\" name=\"login_pass\">\r\n                                                    </div>\r\n                                                    <div class=\"form-actions\">\r\n                                                        <input type=\"submit\" class=\"button button--border\" value=\"{{lang 'login.button' }}\">\r\n                                                        <a class=\"forgot-password\" href=\"{{urls.auth.forgot_password}}\">{{lang 'login.forgot_password' }}</a>\r\n                                                    </div>\r\n                                                </form>\r\n                                            </div>\r\n                                            <div class=\"new-customer\">\r\n                                                {{#if settings.account_creation_enabled}}\r\n                                                    <a class=\"button\" href=\"{{urls.auth.create_account}}\">{{lang 'create_account.button' }}</a>\r\n                                                {{/if}}\r\n                                            </div>\r\n                                        </div>\r\n                                    {{/if}}\r\n                                {{/if}}\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"themevale_header-bottom menu-sticky\">\r\n            <div class=\"logo-wrapper\">\r\n                <div class=\"header-sticky-logo\">\r\n                    <a href=\"{{urls.home}}\">\r\n                        {{#if settings.store_logo.image}}\r\n                            {{#if theme_settings.logo_size '===' 'original'}}\r\n                                <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n                            {{else}}\r\n                                <span class=\"header-logo-image-container\">\r\n                                    <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n                                </span>\r\n                            {{/if}}\r\n                        {{else}}\r\n                            <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n                        {{/if}}\r\n                    </a>\r\n                </div>\r\n            </div>\r\n            {{> components/common/navigation-menu2}}\r\n            {{> components/common/sticky-cart-search}}\r\n        </div>\r\n    </div>\r\n    <div class=\"themevale_header-Mobile\">\r\n        {{> components/themevale/header/header-mobile}}\r\n    </div>\r\n</header>\r\n","components/themevale/header/header-mobile":"<div class=\"header-Mobile-item text-left\">\r\n    <div class=\"items item--hamburger\">\r\n        <a class=\"mobileMenu-toggle\" href=\"#\" data-mobile-menu-toggle=\"menu\">\r\n            <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\r\n        </a>\r\n    </div>\r\n    <div class=\"items item--searchMobile\">\r\n        <a class=\"navUser-action\" href=\"#\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\">\r\n            <div class=\"navUser-icon\">\r\n                <svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>\r\n                <svg class=\"icon icon-close\"><use xlink:href=\"#times\"></use></svg>\r\n            </div>\r\n        </a>\r\n    </div>\r\n</div>\r\n<div class=\"header-Mobile-item text-center\">\r\n    <div class=\"items\">\r\n        <div class=\"logo-wrapper\">\r\n            {{#if page_type '===' 'default'}}\r\n                <h1 class=\"header-logo\">\r\n                    {{> components/common/store-logo}}\r\n                </h1>\r\n            {{else}}\r\n                <div class=\"header-logo\">\r\n                    {{> components/common/store-logo}}\r\n                </div>\r\n            {{/if}}\r\n        </div>\r\n    </div>\r\n</div>\r\n<div class=\"header-Mobile-item text-right\">\r\n    {{#if customer}}\r\n    <div class=\"items item--account\" id=\"mobile-logged\">\r\n        <a class=\"navUser-action accountMobile\" href=\"javascript:void(0)\">\r\n            <div class=\"navUser-icon\">\r\n                    <svg class=\"icon\"><use xlink:href=\"#icon-user\"/></svg>\r\n                    <strong>{{customer.name}}.</strong>\r\n            </div>\r\n        </a>\r\n    </div>\r\n    {{else}}\r\n    <div class=\"items item--account\" id=\"mobile-no-logged\">\r\n        <a class=\"navUser-action accountMobile\" href=\"javascript:void(0)\">\r\n            <div class=\"navUser-icon\">\r\n                <svg class=\"icon\"><use xlink:href=\"#icon-user\"/></svg>\r\n            </div>\r\n        </a>\r\n    </div>\r\n    {{/if}}\r\n    \r\n    <div class=\"items item--cart themevale_cart\">\r\n        <a class=\"navUser-action cartMobile\"\r\n            data-cart-preview\r\n            data-options=\"align:right\"\r\n            href=\"{{urls.cart}}\">\r\n            <div class=\"navUser-icon navUser-item-cartIcon\">\r\n                <svg class=\"icon\"><use xlink:href=\"#icon-cart\"></use></svg>\r\n                <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{#if cart.items}}{{cart.quantity}}{{else}}0{{/if}}</span>\r\n            </div>\r\n        </a>\r\n    </div>\r\n</div>\r\n","components/themevale/header/navigation-4":"<div class=\"container\">\r\n    <div class=\"navUser text-left\">\r\n        <div class=\"navUser-item navUser-item--phone\">\r\n            <a class=\"navUser-action store-phone\" href=\"tel:{{settings.phone_number}}\">\r\n                <svg class=\"icon big\"><use xlink:href=\"#icon-telephone-2\"></use></svg><span class=\"text\">{{settings.phone_number}}</span>\r\n            </a>\r\n        </div>\r\n        <div class=\"navUser-item navUser-item--mail\">\r\n            <a class=\"navUser-action\" href=\"mailto:support@halothemes.com\">\r\n                <svg class=\"icon big\"><use xlink:href=\"#icon-envelope-2\"></use></svg><span class=\"text\">support@halothemes.com</span>\r\n            </a>\r\n        </div>\r\n    </div>\r\n    <div class=\"navUser text-center\">\r\n        <div class=\"navUser-item\"><p class=\"own-text\">FREE DELIVERY WORLDWIDE</p></div>\r\n    </div>\r\n    <div class=\"navUser text-right\">\r\n        <div class=\"navUser-item navUser-item--contact\">\r\n            <a class=\"navUser-action\" href=\"/contact-us/\">need help?</a>\r\n        </div>\r\n        {{#if settings.gift_certificates_enabled}}\r\n        <div class=\"navUser-item\">\r\n            <a class=\"navUser-action\" href=\"{{urls.gift_certificate.purchase}}\">{{lang 'common.gift_cert'}}</a>\r\n        </div>\r\n        {{/if}}\r\n        <div class=\"navUser-item navUser-item--currencySelector\">\r\n            {{> components/common/currency-selector}}\r\n        </div>\r\n    </div>\r\n</div>\r\n"}
